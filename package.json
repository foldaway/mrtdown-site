    "@tanstack/react-query": "^5.66.11",
    "@vercel/analytics": "^1.5.0",
    "classnames": "^2.5.1",
    "html-entities": "^2.6.0",
    "isbot": "^5",
    "luxon": "^3.5.0",
    "posthog-js": "^1.335.0",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intl": "^7.1.10",
    "react-router": "^7.12.0",
